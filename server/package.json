    "@types/multer": "^1.4.11",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.18.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.3",
    "nodemon": "^3.1.0",
    "npm": "^10.5.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-mysql-session": "^3.0.4",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.5",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "concurrently": "^8.2.2",
    "cpx": "^1.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  }
}
