{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "rimraf build && npx tsc",
    "copy-images": "cpx \"src/public/images/**/*\" build/public/images",
    "watch-ts": "npx tsc -w",
    "watch-images": "nodemon -e jpg,png,gif -x \"npm run copy-images\"",
    "prestart": "npm run build && npm run copy-images",
    "start": "concurrently \"npm:watch-ts\" \"npm:watch-images\" \"nodemon build/index.js\"",
    "preserve": "npm run build && npm run copy-images",
    "serve": "concurrently \"npm:watch-ts\" \"npm:watch-images\" \"nodemon build/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
